{
    "name": "omnihive",
    "version": "6.2.26",
    "description": "OmniHive Server",
    "license": "MIT",
    "author": {
        "name": "With One Vision Technologies",
        "url": "https://www.withone.vision",
        "email": "developers@withone.vision"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/WithOneVisionTechnologies/omnihive.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "OmniHive",
        "With One Vision"
    ],
    "main": "./omnihive.js",
    "types": "./omnihive.d.ts",
    "bin": "./omnihive.js",
    "scripts": {
        "build": "tsc",
        "taskRunner:debug": "nodemon --no-stdin --config ./nodemon.taskRunner.json",
        "server:debug": "nodemon --no-stdin --config ./nodemon.server.json"
    },
    "dependencies": {
        "@socket.io/redis-adapter": "7.0.0",
        "@socket.io/redis-emitter": "4.1.0",
        "@withonevision/omnihive-core": "workspace:*",
        "@withonevision/omnihive-worker-config-json": "workspace:*",
        "@withonevision/omnihive-worker-config-mssql": "workspace:*",
        "@withonevision/omnihive-worker-config-mysql": "workspace:*",
        "@withonevision/omnihive-worker-config-postgres": "workspace:*",
        "@withonevision/omnihive-worker-config-sqlite": "workspace:*",
        "@withonevision/omnihive-worker-config-yaml": "workspace:*",
        "@withonevision/omnihive-worker-date-dayjs": "workspace:*",
        "@withonevision/omnihive-worker-encryption-nodeforge": "workspace:*",
        "@withonevision/omnihive-worker-feature-local": "workspace:*",
        "@withonevision/omnihive-worker-filesystem-fsextra": "workspace:*",
        "@withonevision/omnihive-worker-function-core": "workspace:*",
        "@withonevision/omnihive-worker-graphql-builder-v1": "workspace:*",
        "@withonevision/omnihive-worker-knex-mssql": "workspace:*",
        "@withonevision/omnihive-worker-knex-mysql": "workspace:*",
        "@withonevision/omnihive-worker-knex-postgres": "workspace:*",
        "@withonevision/omnihive-worker-knex-sqlite": "workspace:*",
        "@withonevision/omnihive-worker-log-core": "workspace:*",
        "@withonevision/omnihive-worker-server-v1": "workspace:*",
        "@withonevision/omnihive-worker-token-jsonwebtoken": "workspace:*",
        "body-parser": "1.19.0",
        "chalk": "4.1.1",
        "cors": "2.8.5",
        "dotenv": "10.0.0",
        "ejs": "3.1.6",
        "express": "4.17.1",
        "figlet": "1.5.0",
        "fs-extra": "10.0.0",
        "helmet": "4.6.0",
        "node-cleanup": "2.1.2",
        "read-pkg-up": "7.0.1",
        "redis": "3.1.2",
        "serialize-error": "8.1.0",
        "socket.io": "4.1.2",
        "swagger-ui-express": "4.1.6",
        "yaml": "1.10.2",
        "yargs": "17.0.1"
    },
    "devDependencies": {
        "@types/body-parser": "1.19.0",
        "@types/cors": "2.8.10",
        "@types/express": "4.17.12",
        "@types/figlet": "1.5.1",
        "@types/node": "15.6.1",
        "@types/node-cleanup": "2.1.1",
        "@types/redis": "2.8.28",
        "@types/swagger-ui-express": "4.1.2",
        "@types/ws": "7.4.4",
        "@types/yaml": "1.9.7",
        "@types/yargs": "17.0.0",
        "chai": "4.3.4",
        "nodemon": "2.0.7",
        "ts-node": "10.0.0",
        "typescript": "4.2.4"
    },
    "omniHive": {
        "coreDependencies": {
            "@socket.io/redis-adapter": "7.0.0",
            "@socket.io/redis-emitter": "4.1.0",
            "@withonevision/omnihive-core": "workspace:*",
            "@withonevision/omnihive-worker-config-json": "workspace:*",
            "@withonevision/omnihive-worker-config-mssql": "workspace:*",
            "@withonevision/omnihive-worker-config-mysql": "workspace:*",
            "@withonevision/omnihive-worker-config-postgres": "workspace:*",
            "@withonevision/omnihive-worker-config-sqlite": "workspace:*",
            "@withonevision/omnihive-worker-config-yaml": "workspace:*",
            "@withonevision/omnihive-worker-feature-local": "workspace:*",
            "@withonevision/omnihive-worker-filesystem-fsextra": "workspace:*",
            "@withonevision/omnihive-worker-function-core": "workspace:*",
            "@withonevision/omnihive-worker-log-core": "workspace:*",
            "body-parser": "1.19.0",
            "chalk": "4.1.1",
            "cors": "2.8.5",
            "dotenv": "10.0.0",
            "ejs": "3.1.6",
            "express": "4.17.1",
            "figlet": "1.5.0",
            "fs-extra": "10.0.0",
            "helmet": "4.6.0",
            "node-cleanup": "2.1.2",
            "read-pkg-up": "7.0.1",
            "redis": "3.1.2",
            "serialize-error": "8.1.0",
            "socket.io": "4.1.2",
            "swagger-ui-express": "4.1.6",
            "yaml": "1.10.2",
            "yargs": "17.0.1"
        },
        "bootWorkers": [
            {
                "name": "ohBootLogWorker",
                "type": "log",
                "package": "@withonevision/omnihive-worker-log-core",
                "version": "",
                "importPath": "@withonevision/omnihive-worker-log-core/index",
                "default": false,
                "enabled": true,
                "metadata": {}
            }
        ],
        "coreWorkers": [
            {
                "name": "ohreqFeatureWorker",
                "type": "feature",
                "package": "@withonevision/omnihive-worker-feature-local",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-feature-local/index",
                "default": false,
                "enabled": true,
                "metadata": {}
            },
            {
                "name": "ohreqFileSystemWorker",
                "type": "fileSystem",
                "package": "@withonevision/omnihive-worker-filesystem-fsextra",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-filesystem-fsextra/index",
                "default": false,
                "enabled": true,
                "metadata": {}
            },
            {
                "name": "ohreqRestSystemAccessToken",
                "type": "restEndpointFunction",
                "package": "@withonevision/omnihive-worker-function-core",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-function-core/SystemAccessTokenWorker",
                "default": false,
                "enabled": true,
                "metadata": {
                    "restMethod": "post",
                    "urlRoute": "token"
                }
            },
            {
                "name": "ohreqRestSystemRegister",
                "type": "restEndpointFunction",
                "package": "@withonevision/omnihive-worker-function-core",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-function-core/SystemRegisterWorker",
                "default": false,
                "enabled": true,
                "metadata": {
                    "restMethod": "post",
                    "urlRoute": "register"
                }
            },
            {
                "name": "ohreqRestSystemRefresh",
                "type": "restEndpointFunction",
                "package": "@withonevision/omnihive-worker-function-core",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-function-core/SystemRefreshWorker",
                "default": false,
                "enabled": true,
                "metadata": {
                    "restMethod": "post",
                    "urlRoute": "refresh"
                }
            },
            {
                "name": "ohreqRestSystemStatus",
                "type": "restEndpointFunction",
                "package": "@withonevision/omnihive-worker-function-core",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-function-core/SystemStatusWorker",
                "default": false,
                "enabled": true,
                "metadata": {
                    "restMethod": "post",
                    "urlRoute": "status"
                }
            }
        ],
        "defaultWorkers": [
            {
                "name": "defaultBuildWorker",
                "type": "graphBuilder",
                "package": "@withonevision/omnihive-worker-graphql-builder-v1",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-graphql-builder-v1/index",
                "default": true,
                "enabled": true,
                "metadata": {
                    "dbWorkers": [
                        "*"
                    ],
                    "urlRoute": "builder1"
                }
            },
            {
                "name": "defaultDateWorker",
                "type": "date",
                "package": "@withonevision/omnihive-worker-date-dayjs",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-date-dayjs/index",
                "default": true,
                "enabled": true,
                "metadata": {
                    "dateFormat": "YYYY-MM-DD hh:mm:ssA"
                }
            },
            {
                "name": "defaultEncryptionWorker",
                "type": "encryption",
                "package": "@withonevision/omnihive-worker-encryption-nodeforge",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-encryption-nodeforge/index",
                "default": true,
                "enabled": true,
                "metadata": {
                    "encryptionKey": "${ohEncryptionKey}"
                }
            },
            {
                "name": "defaultServerWorker",
                "type": "server",
                "package": "@withonevision/omnihive-worker-server-v1",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-server-v1/index",
                "default": true,
                "enabled": true,
                "metadata": {
                    "buildWorkers": [
                        "*"
                    ],
                    "urlRoute": "server1"
                }
            },
            {
                "name": "defaultTokenWorker",
                "type": "token",
                "package": "@withonevision/omnihive-worker-token-jsonwebtoken",
                "version": "workspace:*",
                "importPath": "@withonevision/omnihive-worker-token-jsonwebtoken/index",
                "default": true,
                "enabled": true,
                "metadata": {
                    "audience": "${ohTokenAudience}",
                    "tokenSecret": "${ohTokenSecret}",
                    "expiresIn": "${ohTokenExpiresIn}",
                    "hashAlgorithm": "${ohTokenHashAlgorithm}",
                    "verifyOn": false
                }
            }
        ]
    }
}
